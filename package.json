{
  "name": "request-pure",
  "version": "1.1.0",
  "description": "Zero-dependency,lightweight http request client",
  "keywords": [
    "http",
    "https",
    "request",
    "curl",
    "wget",
    "fetch",
    "net",
    "network",
    "gzip",
    "brotli",
    "requests",
    "human-friendly"
  ],
  "homepage": "https://github.com/islizeqiang/request-pure#readme",
  "bugs": {
    "url": "https://github.com/islizeqiang/request-pure/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/islizeqiang/request-pure.git"
  },
  "license": "MIT",
  "author": "islizeqiang",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.es.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "siroc build --watch",
    "build": "del dist && siroc build",
    "release": "npm run build && standard-version && git push --follow-tags && npm publish",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": "eslint --ext .js,.ts,.vue",
    "**/*.{js,ts,md,json}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "code-fabric": "^1.2.0",
    "del-cli": "^3.0.1",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "pinst": "^2.1.6",
    "siroc": "^0.8.0",
    "standard-version": "^9.1.1"
  }
}
